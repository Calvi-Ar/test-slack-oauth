# Slack OAuth Configuration
# Get these values from your Slack app settings at https://api.slack.com/apps

# Your Slack app's Client ID
NEXT_PUBLIC_SLACK_CLIENT_ID=your_slack_client_id_here

# Your Slack app's Client Secret (server-side only)
SLACK_CLIENT_SECRET=your_slack_client_secret_here

# OAuth Redirect URI (should match what you configure in Slack app settings)
NEXT_PUBLIC_SLACK_REDIRECT_URI=https://your-domain.vercel.app/api/auth/callback
SLACK_REDIRECT_URI=https://your-domain.vercel.app/api/auth/callback

# Environment
NODE_ENV=development 